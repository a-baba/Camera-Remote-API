{
  "name": "Video Live Cam Addendum to Presentation API",
  "version": "0.0",
  "manifest_version": 2,
  "description": "PoC implementation for Video Live Cam Addendum Extention",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:28888/*"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
